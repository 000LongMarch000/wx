<div class="module-tree_selector">
    <div class="body">
        <div class="tree tree-0">
            {{#with tree0}}
            <div class="tree-title">{{title}}</div>
            <div class="tree-data"></div>
            {{/with}}
        </div>
        <div class="tree tree-1">
            {{#with tree1}}
            <div class="tree-title">{{title}}</div>
            <div class="tree-data"></div>
            {{/with}}
        </div>
        <div class="actions">
            <a href="#" class="J_buttonAdd btn btn-large">添加 >></a>
        </div>
    </div>
    <div class="footer"><a href="#" class="J_buttonConfirm btn btn-primary btn-large">确定</a><a href="#" class="J_buttonCancel btn btn-large">取消</a></div>
</div>